{
  "version": 2,
  "builds": [
    {
      "src": "server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "db.json"
        ]
      }
    },
    {
      "src": "build/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "build/$1"
    }
  ]
}